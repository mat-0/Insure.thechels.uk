{% assign contents_of_page = site.pages | where: "name", "index.md" | first %}
{% assign pub_date = site.time | date: "%Y-%m-%d" %}

<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>Daily Rundown</title>
     <link>>https://insure.thechels.uk</link>
     <description>List of open issues for insure.thechels.uk</description>
    <item>
      <title>Daily Rundown for {{ pub_date }}</title>
      <link>https://insure.thechels.uk/#{{ pub_date }}</link>
      <guid isPermaLink="false">Daily Summary for {{ pub_date }}</guid>
      <pubDate>{{ pub_date }}</pubDate>
      <description>
      <![CDATA[ {{ contents_of_page.content | markdownify }} ]]>
      </description>
    </item>
  </channel>
</rss>
